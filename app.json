{
  "expo": {
    "name": "Fit Pro",
    "slug": "fit-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fitpro-logo-letter-nobg.png",
    "scheme": "fit-pro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fitpro.app",
      "buildNumber": "0.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fitpro-logo-letter-bw-nobg.png",
        "backgroundColor": "#0f766e"
      },
      "package": "com.fitpro.app",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/fitpro-logo-letter-nobg.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4a060f33-e72c-488e-8d70-3abaad786442"
      }
    }
  }
}